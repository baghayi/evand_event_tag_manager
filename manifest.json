{
    "name": "Tag Manager",
    "version": "1.0",
    "description": "Evand Event Tag Manager",
    "permissions": ["https://api.evand.com/", "tabs", "storage", "https://evand.com/"],
    "browser_action": {
    },
    "background": {
        "scripts": [
            "tasks/StartingScreenSelector.js"
        ],
        "persistent": false
    },
    "manifest_version": 2,
    "commands": {
        "_execute_browser_action": {
            "suggested_key": {
                "linux": "Alt+C"
            }
        }
    }
}
